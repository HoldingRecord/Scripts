local Target = "vvmatthxw"

local LocalPlayer = game.Players.LocalPlayer
local Character = LocalPlayer.Character
local ClonedPlayer = Character.Humanoid:Clone()
amongus = game:GetService('Players').LocalPlayer
game.Players[Target].Character.Humanoid:SetStateEnabled("Seated", true)
game.Players[Target].Character.Humanoid.Sit = false
prev = amongus.Character:WaitForChild('HumanoidRootPart').CFrame
ClonedPlayer.Parent = Character
Character.Humanoid:Destroy()

for i, v in next, LocalPlayer.Backpack:GetChildren() do
    if v:IsA("Tool") then
        v.Parent = Character
    end
end

local Tool = Character:FindFirstChildOfClass("Tool")

firetouchinterest(Tool.Handle, game.Players[Target].Character["Head"], 0)

game:GetService("RunService").Heartbeat:wait()

game.Workspace['FallenPartsDestroyHeight'] = 0 / 0
game.Workspace['Gravity'] = 1e5
Character["HumanoidRootPart"].CFrame = Character["HumanoidRootPart"].CFrame - Vector3.new(0, 1e5, 0)
Character.HumanoidRootPart.Velocity = Vector3.new(0, -9e9, 0)

wait(0.20)
Character:Destroy()
game.Players.LocalPlayer.CharacterAdded:Wait()
amongus = game.Players.LocalPlayer.Character
amongus:WaitForChild("Humanoid")
amongus:WaitForChild('HumanoidRootPart').CFrame = prev
